  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "scope": [
        "keyword",
        "support.type.property-name.json",
        "string.unquoted.plain.out.yaml"
      ],
      "settings": {
        "foreground": "#93289f"
      }
    },
    {
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#005cc5"
      }
    },
    {
      "scope": [
        "string",
        "constant.language.boolean",
        "constant.numeric",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#22863a"
      }
    }
  ],
  "semanticTokenColors": {
    "interface": "#c24e00",
    "typeParameter": "#0097a7",
    "lifetime": "#0097a7",
    "namespace": "#000",
    "enumMember": {
      "foreground": "#000",
      "fontStyle": "italic"
    },
    "*.attribute": "#b08800",
    "*.mutable": {
      "fontStyle": "underline"
    }
  },
  "semanticHighlighting": true
}